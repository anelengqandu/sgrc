@{
}

<script src="~/scripts/_program/program.js"></script>


<div class="col-md-12">
    <div class="tabbable-line boxless tabbable-reversed">
        <div class="row">
            <div class="col-md-12">
                <div class="portlet box blue-hoki">
                    <div class="portlet-title">
                        <div class="caption">
                            <i class="fa fa-gear"></i>Create Program
                        </div>
                        <div class="tools">
                            <a href="javascript:;" class="collapse" data-original-title="" title=""> </a>
                            
                            <a href="javascript:;" onclick="reaload()" class="reload" data-original-title="" title=""> </a>
                            
                        </div>
                    </div>
                    <div class="portlet-body form">
                        <!-- BEGIN FORM-->
                        <form action="#">
                            <div class="form-actions top">
                                <button type="button" onclick="clear()" class="btn default pull-right" >Cancel</button>
                                <button type="button" onclick="onRegisterProgram()" class="btn green pull-right" style="margin-right: 4px;">Submit</button>

                            </div>
                            <div class="form-body">
                                <div class="form-group ">
                                    <label class="control-label">Program Name</label>
                                    <input type="text" id="txtName" name="Name" class="form-control" placeholder="Program Name">
                                </div>
                                <div class="form-group">
                                    <label class="control-label">Program Description</label>

                                    <input type="text" id="txtDescription" name="Description" class="form-control" placeholder="Program Description">

                                </div>

                            </div>
                            <div class="form-actions">
                                <button type="button" onclick="clear()" class="btn default pull-right">Cancel</button>
                                <button type="button" onclick="onRegisterProgram()" class="btn green pull-right" style="margin-right: 4px;">Submit</button>

                            </div>
                        </form>
                        <!-- END FORM-->
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>
